                "disks": [
                    {
                        "mount": "/user/cache1",
                        "quota": 100,
                        "quotaUnit": "size"
                    }
                ],
                "cleanup": {
                    "time": "02:00",
                    "age": 1,
                    "emptyDir": "delete",
                    "fileCount": 0
                }
            }
        }
