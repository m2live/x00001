                "memoryRatio": 100,
                "memoryRatioFree": 40,
                "memoryRatioContents": 50,
                "cacheObjectSize": 900,
                "enableEmergencyMode": false,
                "enableSyncStale": true,
                "writeCountSmallFile": 0
